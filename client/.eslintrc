{
  "extends": "standard",
  "globals": {
    "Phaser": true
  },
  "rules": {
    "no-extra-semi": 0,
    "semi": 0,
    "key-spacing": 0,
    "indent": [2, 4, {
      "SwitchCase": 1
    }],
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "always"
    }]
  }
}